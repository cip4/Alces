<!-- first message, announcing first   preview available  -->
<!--	 KVDP Agfa 050321 derived from resourcecmd and platemaking sample 
	 KVDP 050405 adapted after ConvPrint meeting: added parts, removed intermediate statuses
	KVDP 050815 improved ID and TimeStamp to be more realistic; refID=JobID
	KVDP 050920 add ResourceQuParams to transfer Part and Job related info; make more realistic by signaling only one preview at  a time
   KVDP 051208 added Exact="true", matched ids and names with platemaking sample
 -->
<JMF SenderID="MISPrepress" TimeStamp="2005-03-18T08:31:31+01:00" Version="1.2" xmlns="http://www.CIP4.org/JDFSchema_1_1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!-- generated by (in NodeInfo platemaking sample)
			<Query Type="Resource" xsi:type="QueryResource" ID="QUERY_Resource_PlateMaking">
				<ResourceQuParams Classes="Parameter" Usage="Output" Exact="true" JobID="MISPRE_Platemaking"/>
				<Subscription URL="http://www.misprepress.org/jmfserver"/>
			</Query>
-->
	<Signal ID="m083208_0600" Type="Resource" refID="QUERY_Resource_PlateMaking" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:type="SignalResource">
		<!-- KVDP 20050920 ResourceQuParams added after clarification by Rainer to allow:
                    - specify Part also with JDF 1.3 (removed from ResourceInfo)
					- specify JobID, JobPartID, ResourceName, ProcessUsage (similar to CommandResource but JobPartID is not allowed there by PRECP ICS
		-->
		<ResourceQuParams ResourceName="Preview" ProcessUsage="Plate" JobID="MISPRE_PlateMaking" JobPartID="IMS000" Exact="true">
			<!-- Cyan available -->
			<Part SignatureName="SIG1059600001" SheetName="SHT1059600001" Side="Front" Separation="Cyan"/>
		</ResourceQuParams>
		<ResourceInfo>
			<!-- ResourceName attribute not allowed since we send real resource 
      				MIS ICS likes ActualAmount but Preview is not a Consumable, it is a Parameter resource
			-->
			<Preview Class="Parameter" ID="Preview01" PartIDKeys="SignatureName SheetName Side Separation" PartUsage="Explicit" PreviewUsage="Separation" PreviewFileType="PNG" Compensation="Plate" Status="Unavailable">
				<Preview SignatureName="SIG1059600001">
					<Preview SheetName="SHT1059600001">
						<Preview Side="Front">
							<Preview Separation="Cyan" Status="Available" URL="file:src/test/resources/org/cip4/tools/alces/data/PreviewTestTest/preview1.png"/>
							<!-- need to supply URL to satisfy CheckJDF (but schema then complains) -->
							<Preview Separation="Black" URL=""  />
							<Preview Separation="Yellow" URL=""  />
							<Preview Separation="Magenta" URL=""  />
						</Preview>
					</Preview>
				</Preview>
			</Preview>
		</ResourceInfo>
	</Signal>
</JMF>
